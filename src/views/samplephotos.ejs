<!doctype html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>The Aps-c</title>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx" crossorigin="anonymous">
</head>

<body class="vh-100 bg-dark">
  <div class="d-flex flex-row flex-wrap">

    <% for(var i = 0; i<samplephotos.length; i++){ %>
      <a href="/lens/<%= samplephotos[0].lens.toString() %>/samplephoto/<%= samplephotos[i]._id.toString() %>">

        <div style="height: 170px; width: auto;" class="m-2 position-relative">
          <img src="<%= samplephotos[i].url %> " class="h-100">
          <div class="position-absolute bottom-0 end-0 bg-opacity-75 bg-dark p-3">
            <p class="p-0 m-0 text-light">ID: <%= samplephotos[i].uploader %> </p>
            <p class="p-0 m-0 text-white-50"><%= samplephotos[i].description %> </p>
          </div>
        </div>
      </a>
    <% } %>
  </div>
</body>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/js/bootstrap.bundle.min.js"
  integrity="sha384-A3rJD856KowSb7dwlZdYEkO39Gagi7vIsF0jrRAoQmDKKtQBHUuLZ9AsSv4jD4Xa" crossorigin="anonymous"></script>
<script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>

</html>